pragma solidity ^0.8.17;

import "./BasicCompliance.sol";
import "fhevm/lib/TFHE.sol";
import "../../token/IToken.sol";

contract DefaultCompliance is BasicCompliance {
    /**
     *  @dev See {ICompliance-transferred}.
     */
    // solhint-disable-next-line no-empty-blocks
    function transferred(address _from, address _to, euint64 _value) external override returns (ebool) {
        ebool isTransferred = TFHE.asEbool(true);
        TFHE.allowTransient(isTransferred, msg.sender);
        return isTransferred;
    }

    /**
     *  @dev See {ICompliance-created}.
     */
    // solhint-disable-next-line no-empty-blocks
    function created(address _to, euint64 _value) external override returns (ebool) {
        ebool isCreated = TFHE.asEbool(true);
        TFHE.allowTransient(isCreated, msg.sender);
        return isCreated;
    }

    /**
     *  @dev See {ICompliance-destroyed}.
     */
    // solhint-disable-next-line no-empty-blocks
    function destroyed(address _from, euint64 _value) external override {}

    /**
     *  @dev See {ICompliance-canTransfer}.
     */
    function canTransfer(address /*_from*/, address /*_to*/, euint64 _value) external override returns (ebool) {
        ebool isTransferrable = TFHE.asEbool(true);
        TFHE.allowTransient(isTransferrable, msg.sender);
        return isTransferrable;
    }

    function getModules() external view returns (address[] memory _modules) {}
}
